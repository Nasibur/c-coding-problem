//Get any kind of positive number from user and print the closets value of the square root of the number.

#include<bits/stdc++.h>
using namespace std;


int main(){

   float a;
   cin>>a;
   double b= sqrt(a);
   int c= sqrt(a);
   double d = b-c;
   if (d>0.5){
     cout<< ++c;
   }
   else if(d<0.5){
    cout << --c;
   }
   else if(d==0.5) {
        cout << c;
   }
   else {
    return -1;
   }

    return 0;
}
